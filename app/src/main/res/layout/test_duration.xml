<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.sdsmdg.harjot.crollerTest.Croller
        android:id="@+id/croller"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_centerInParent="true"
        app:is_continuous="false"
        app:progress_secondary_color="@color/md_green_100"
        app:progress_primary_color="@color/colorAccentAlternative"
        app:back_circle_color="#EDEDED"
        app:main_circle_color="#FFFFFF"
        app:indicator_color="@color/colorAccentAlternative"
        app:indicator_width="10"
        app:progress_secondary_circle_size="5"
        app:progress_primary_circle_size="10"
        app:label="10 min"
        app:label_color="@color/md_black" />
</RelativeLayout>
